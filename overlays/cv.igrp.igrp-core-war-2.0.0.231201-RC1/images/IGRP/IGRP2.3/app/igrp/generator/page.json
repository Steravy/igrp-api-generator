"js":"$.IGRP.on('init', function() {\n  $('table > tbody  > tr').each(function(index, tr) {\n    $(this).find('td').each (function() {\n      var checkbox = $(this).find(\"input:checkbox\");\n      var checkValue = checkbox.val();\n      \n      if(checkValue=='0'){\n        $(this).html(\"\");\n      }\n    });\n  });\n                                \n  var checkCuidados = function(){\n    \n    var checks = $('input[name=\"p_cuidados_check\"]').val();\n\n    if(checks != '') {\n      var count = 1;\n      var arrDet = checks.split(\";\");\n\n      $('table > tbody  > tr').each(function(index, tr) { \n\n        $(this).find('td').each (function() {\n          var checkbox = $(this).find(\"input:checkbox\");\n          checkbox.prop('checked', false).removeAttr('checked');\n          var checkValue = checkbox.val();\n          var inputNameCheckFk= 'p_'+ $(this).attr('item-name') + '_check_fk';\n          var inputNameFk= 'p_'+ $(this).attr('item-name') + '_fk';\n\n          if (checkValue != ''){\n            if(jQuery.inArray(checkValue, arrDet) !== -1) {\n              checkbox.attr('checked', 'checked');  \n              $(this).find(\"input[name='\"+inputNameCheckFk+\"']\").val(checkValue);\n              $(this).find(\"input[type='hidden'][name='\"+inputNameFk+\"']\").remove();\n              count++;\n            }  \n          }\n\n        });\n\n      });\n    }\n    \n  };\n  \n  setTimeout(function(){\n    var diagnostico_url = $('input[name=\"p_pesquisa_diagnostico\"]').val();\n    $('#informacoes_emame_diagnostico').select2({\n      \tminimumInputLength: 2,\n      \tplaceholder: 'Pesquise aqui pelo diagnóstico',\n  \t\tajax: {\n            url: diagnostico_url,\n            dataType: 'json',\n            type: 'GET',\n            data: function (params) {\n              \n                return {\n                    p_find: params.term,\n                };\n            },\n            processResults: function (response) {\n                return {\n                    results: response\n                    }\n            },\n          \tcache: true\n  \t\t}\n\t});\n  },1000);\n  \n  $('select[name=\"p_agrupamento\"]').change(function(){\n    var agrupamento = $('select[name=\"p_agrupamento\"]').val();\n\n    if(!agrupamento || agrupamento == ''){\n      $('table > tbody  > tr').each(function(index, tr) { \n        var checkbox = $(this).find(\"input:checkbox\");\n        checkbox.prop('checked', false).removeAttr('checked');\n      })\n    }\n\n  });\n  \n  $('input[name=\"p_cuidados_check\"]').change(function(){\n    checkCuidados();    \n  });\n  \n  var taskConfirmar = $('input[name=\"p_confirmar\"]').val();\n  if(taskConfirmar == 'true')\n     checkCuidados();\n});","files":{"css":[],"js":[],"xsl":[{"file":"components/2.3/GEN-table.tmpl.xsl"}]},"service":{},"blocklyData":""}